    "class User:\n",
    "    \"\"\"Classe repr√©sentant un utilisateur de la biblioth√®que.\"\"\"\n",
    "    \n",
    "    def __init__(self, name, user_id):\n",
    "        \"\"\"\n",
    "        Initialise un nouvel utilisateur.\n",
    "        \n",
    "        Args:\n",
    "            name (str): Nom de l'utilisateur\n",
    "            user_id (int): Identifiant unique de l'utilisateur\n",
    "        \"\"\"\n",
    "        self.name = name\n",
    "        self.user_id = user_id\n",
    "        self.borrowed_books = []\n",
    "    \n",
    "    def __str__(self):\n",
    "        \"\"\"Retourne une repr√©sentation en cha√Æne de l'utilisateur.\"\"\"\n",
    "        return f\"User: {self.name} (ID: {self.user_id}), Books borrowed: {len(self.borrowed_books)}\"\n",
    "    \n",
    "    def borrow_book(self, book):\n",
    "        \"\"\"\n",
    "        Emprunte un livre si celui-ci est disponible et si l'utilisateur\n",
    "        n'a pas d√©j√† emprunt√© 3 livres.\n",
    "        \n",
    "        Args:\n",
    "            book (Book): Le livre √† emprunter\n",
    "            \n",
    "        Returns:\n",
    "            bool: True si l'emprunt est r√©ussi, False sinon\n",
    "        \"\"\"\n",
    "        if book.available and len(self.borrowed_books) < 3:  # Limitation √† 3 livres\n",
    "            book.borrow()  # Marque le livre comme emprunt√©\n",
    "            self.borrowed_books.append(book)\n",
    "            return True\n",
    "        return False\n",
    "    \n",
    "    def return_book(self, book):\n",
    "        \"\"\"\n",
    "        Retourne un livre emprunt√©.\n",
    "        \n",
    "        Args:\n",
    "            book (Book): Le livre √† retourner\n",
    "            \n",
    "        Returns:\n",
    "            bool: True si le retour est r√©ussi, False sinon\n",
    "        \"\"\"\n",
    "        if book in self.borrowed_books:\n",
    "            book.return_book()  # Marque le livre comme disponible\n",
    "            self.borrowed_books.remove(book)\n",
    "            return True\n",
    "        return False\n",
    "\n",
