    "class Book:\n",
    "    \"\"\"Classe repr√©sentant un livre dans la biblioth√®que.\"\"\"\n",
    "    \n",
    "    def __init__(self, title, author, isbn, available=True):\n",
    "        \"\"\"\n",
    "        Initialise un nouveau livre.\n",
    "        \n",
    "        Args:\n",
    "            title (str): Titre du livre\n",
    "            author (str): Auteur du livre\n",
    "            isbn (str): Num√©ro ISBN du livre\n",
    "            available (bool, optional): Disponibilit√© du livre. Par d√©faut √† True.\n",
    "        \"\"\"\n",
    "        self.title = title\n",
    "        self.author = author\n",
    "        self.isbn = isbn\n",
    "        self.available = available\n",
    "    \n",
    "    def __str__(self):\n",
    "        \"\"\"Retourne une repr√©sentation en cha√Æne du livre.\"\"\"\n",
    "        return f\"{self.title} by {self.author} (ISBN: {self.isbn})\"\n",
    "    \n",
    "    def borrow(self):\n",
    "        \"\"\"Marque le livre comme emprunt√© (non disponible).\"\"\"\n",
    "        if self.available:\n",
    "            self.available = False\n",
    "            return True\n",
    "        return False\n",
    "    \n",
    "    def return_book(self):\n",
    "        \"\"\"Marque le livre comme retourn√© (disponible).\"\"\"\n",
    "        if not self.available:\n",
    "            self.available = True\n",
    "            return True\n",
    "        return False\n",
    "\n",
