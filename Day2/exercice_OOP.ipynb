    "\n",
    "# Alice emprunte \"Python Programming\"\n",
    "user1.borrow_book(book1)\n",
    "\n",
    "# Affichage des livres emprunt√©s (affiche des objets Book, pas des cha√Ænes)\n",
    "print(\"Livres emprunt√©s:\")\n",
    "for book in user1.borrowed_books:\n",
    "    print(f\"- {book}\")\n",
    "\n",
    "# V√©rification de l'√©tat d'emprunt\n",
    "print(f\"Nombre de livres emprunt√©s: {len(user1.borrowed_books)}\")\n",
    "print(f\"√âtat du livre: {'Emprunt√©' if not book1.available else 'Disponible'}\")\n",
    "\n",
    "# Alice retourne le livre\n",
    "user1.return_book(book1)\n",
